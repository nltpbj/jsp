<script>
	getData();
	function getData(){
	   $.ajax({
	      type:"get",
	      url:"https://dapi.kakao.com/v3/search/book?target=title",
	      headers:{"Authorization":"KakaoAK f757bb64a262baafa17a02a6042a9d76"},
	      dataType:"json",
	      data:{"query":"JSP", page:1, size:6},
	      success:function(data){
	         console.log(data);
	      }
	   });
	}
</script>